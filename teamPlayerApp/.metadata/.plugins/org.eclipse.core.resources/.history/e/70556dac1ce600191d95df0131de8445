package bus;

public class Member 
{
	private int id;
	private String firstName;
	private String lastName;
	private PersonnelType type;
	private double salary;
	
	//Default constructor
	public Member() 
	{
		this.setType(PersonnelType.Undefined);
		this.setSalary(0.00);
	}
	
	public Member(int id, String fn, String ln, PersonnelType type, double salary)
	{
		this.setId(id);
		this.setFirstName(fn);
		this.setLastName(ln);
		this.setType(type);
		this.setSalary(salary);
	}

	public int getId() 
	{
		return id;
	}

	public void setId(int id) 
	{
		this.id = id;
	}

	public String getFirstName() 
	{
		return firstName;
	}

	public void setFirstName(String firstName)
	{
		this.firstName = firstName;
	}

	public String getLastName() 
	{
		return lastName;
	}

	public void setLastName(String lastName) 
	{
		this.lastName = lastName;
	}

	public PersonnelType getType() 
	{
		return type;
	}

	public void setType(PersonnelType type) 
	{
		this.type = type;
	}

	public double getSalary() 
	{
		return salary;
	}

	public void setSalary(double salary) 
	{
		this.salary = salary;
	}
	
	
}
