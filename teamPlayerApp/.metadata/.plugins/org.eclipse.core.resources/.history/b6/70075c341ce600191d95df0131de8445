package bus;

public class Member 
{
	private int id;
	private String firstName;
	private String lastName;
	private PersonnelType type;
	private double salary;
	
	//Default constructor
	public Member() 
	{
		this.type = PersonnelType.Undefined;
		this.salary = 0.00;
	}
	
	public Member(int id, String fn, String ln, PersonnelType type, double salary)
	{
		this.id = id;
		this.firstName = fn;
		this.lastName = ln;
		this.type = type;
		this.salary = salary;
	}
}
